<h2> add a concert</h2>

<%= form_for @concert do |f| %>
	<%= f.label :artist %>
	<%= f.text_field :artist %>
	<br>
	<%= f.label :venue %>
	<%= f.text_field :venue %>
	<br>	
	<%= f.label :city %>
	<%= f.text_field :city %>
	<br>
	<%= f.label :date %>
	<%= f.datetime_local_field :date %>
	<br>
	<%= f.label :price %>
	<%= f.text_field :price %>
	<br>
	<%= f.label :description %>
	<%= f.text_field :description %>
	<br>
	<%= f.submit "Create" %>
<% end %>